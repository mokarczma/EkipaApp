@model IEnumerable<Ekipa.Models.DB.Reservation>

@{
    ViewBag.Title = "MyReservation";
}

<h2>MyReservation</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.DescriptionCustomer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DescriptionCompany)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompanyAccept)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsDelete)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
     
        <td>
            @Html.DisplayFor(modelItem => item.DescriptionCustomer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DescriptionCompany)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyAccept)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsDelete)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
